{
  "name": "talbati",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.0.0",
    "yarn": ">= 1.21.1"
  },
  "scripts": {
    "start": "expo start -c",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "eject": "expo eject",
    "expo-optimize": "npx expo-optimize",
    "update": "eas update",
    "update:dev": "eas update --branch development",
    "update:prod": "eas update --branch production",
    "build-dev:and": "eas build --clear-cache --profile=development -p android",
    "build-dev:ios": "eas build --clear-cache --profile=development -p ios",
    "build-dev:all": "eas build --clear-cache --profile=development -p all",
    "build:and": "eas build --clear-cache --profile=production -p android",
    "build:ios": "eas build --clear-cache --profile=production -p ios",
    "build:all": "eas build --clear-cache --profile=production -p all",
    "submit:all": "eas submit -p all --latest",
    "submit:ios": "eas submit -p ios --latest",
    "submit:and": "eas submit -p android --latest"
  },
  "dependencies": {
    "@expo-google-fonts/tajawal": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-masked-view/masked-view": "0.2.7",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/material-top-tabs": "^6.2.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "expo": "~46.0.9",
    "expo-app-loading": "~2.1.0",
    "expo-constants": "~13.2.4",
    "expo-dev-client": "~1.2.1",
    "expo-document-picker": "~10.3.0",
    "expo-font": "~10.2.0",
    "expo-image-picker": "~13.3.1",
    "expo-localization": "~13.1.0",
    "expo-notifications": "~0.16.1",
    "expo-secure-store": "~11.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-updates": "~0.14.5",
    "firebase": "9.1.0",
